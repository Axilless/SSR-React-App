{
  "version": 2,
  "buildCommand": "npm run build:production",
  "devCommand": "npm run start",
  "installCommand": "npm install",
  "outputDirectory": "dist",

  "builds": [
    {
      "src": "/dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
